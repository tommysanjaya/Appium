<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	
<groups>
	<run>
		<include name="Smoke"></include>
	</run>
</groups>
	
<listeners>
	<listener class-name="org.Appium.TestUtils.Listeners"/>
</listeners>
	
  <test thread-count="5" name="End to End Checkout">
    <classes>
      <class name="org.Appium.checkout"/>
    </classes>
  </test> <!-- Test -->
  
    <test thread-count="5" name="End to End Checkout Experiment">
    <classes>
      <class name="org.Appium.checkoutExperimen"/>
    </classes>
  </test> <!-- Test -->
  
      <test thread-count="5" name="Error Validation">
    <classes>
      <class name="org.Appium.errorValidation"/>
    </classes>
  </test> <!-- Test -->
  
  
  
</suite> <!-- Suite --> 
